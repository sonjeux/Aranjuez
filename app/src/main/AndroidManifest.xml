<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aranjuez">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".PreventaProductoActivity"
            android:label="Productos"
            />
        <activity
            android:name=".PreventaActivity"
            android:label="Preventa"
            android:parentActivityName=".ClienteListadoActivity" />
        <activity
            android:name=".ClienteListadoActivity"
            android:label="Listado de Clientes"
            android:parentActivityName=".PreventaListadoActivity" />
        <activity
            android:name=".PreventaListadoActivity"
            android:label="Listado de Preventas"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ProductoListadoActivity"
            android:label="Listado de Productos"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".JsonActivity"
            android:label="Importar Datos"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".configuracion"
            android:label="Configuracion Servicio"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>